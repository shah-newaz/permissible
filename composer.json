{
  "name": "shah-newaz/permissible",
  "description": "Role Permission system for Laravel 9",
  "keywords": [
    "laravel",
    "admin",
    "backend",
    "ACL",
    "Role",
    "Permission",
    "Role Permission"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Shahnewaz Rifat",
      "email": "rifat_rifats@live.com"
    }
  ],
  "require": {
    "php": "^8.0.2",
    "guzzlehttp/guzzle": "^7.2",
    "laravel/framework": ">=v8.0 || >=9.0",
    "firebase/php-jwt": "^6.1",
    "php-open-source-saver/jwt-auth": "^1.4"
  },
  "autoload": {
    "classmap": [
      "database",
      "database/migrations",
      "database/seeds"
    ],
    "psr-4": {
      "Shahnewaz\\Permissible\\": "src/"
    },
    "files": [
      "src/helpers/general.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Shahnewaz\\Permissible\\Providers\\PermissibleServiceProvider"
      ],
      "aliases": {
        "Permissible": "Shahnewaz\\Permissible\\Facades\\PermissibleFacade"
      }
    }
  }
}
